    .circle-carousel
        position relative
        display flex
        height 80vh
        margin-top 150px
        .slidesT
            position relative
            width 50%
            display flex
            padding-left 60px
            .slides
                position relative
                width 80%
                height 100%
                .slide
                    padding-top 22%
                    padding-left 10%
                    padding-right 10%
