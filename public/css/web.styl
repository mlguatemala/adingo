body
    background rgb(252,252,252)
    margin 0px
    font-family font-book
    overflow-x hidden !important

.sec1, .sec2, .sec3, .sec4, .sec5, .sec6, .sec7, .sec8, .sec9, .sec10, .sec11, .sec12, .sec13, .sec14, .sec15, .sec16, .sec17
    position relative

.loading
    position fixed
    width 100vw
    height 120vh
    background rgb(0,0,0,1)
    display flex
    justify-content center
    align-items center
    z-index 999999
    top 0
    #loader
        border 2px #3F3F3F solid
        border-radius 50%
        border-top-color #F26E34
        width 90px
        height 90px
        animation spin 1s linear infinite
        z-index 9999
        @keyframes spin
            from
                transform translate(0%, -50%) rotate(0deg)
            to
                transform translate(0%, -50%) rotate(360deg)

#header //header
    background #000
    height 10vh
    position fixed
    z-index 9
    width 100%
    transition 1s
    top 0
    display flex
    align-items center
    z-index 999
    .left-menu
        img.img-logo
            height 40px
    .right-menu
        .burger0
            float right
            display block
            cursor pointer
            color #00AC67
            .linea1, .linea2, .linea3
                width 40px
                height 4px
                background-color #F26E34
                margin 5px
                transition all .3s ease
        .toggle1
            transform rotate(-45deg) translate(-6.5px,6.5px)
        .toggle2
            opacity 0
        .toggle3
            transform rotate(45deg) translate(-5.5px,-6.5px)
    .fondo-active
        background rgba(0, 0, 0, .7) !important
        display block !important
    .fondo
        background rgba(0, 0, 0, 0)
        position fixed
        display none
        height 100%
        width 100%
        top 0
        left 0
        z-index 9
        transition transform .3s ease-in
    .nav-mobile-active
        transform translateX(0%) !important
    .nav-mobile
        display block
        position fixed
        top 0
        right 0
        background #F26E34
        height 100vh
        width 30%
        z-index 9999999999
        padding 90px 40px 30px 30px
        transform translateX(100%)
        transition transform .3s ease-in
        z-index 10
        .lists
            display flex
            flex-direction column
            height 80vh
            justify-content space-between
            ul
                list-style none
                padding-left 15px
                li
                    padding-bottom 17px
                    a
                        text-decoration none
                        color #fff
                        font-size 1.7rem
                        font-family font-medium
            .bottom_
                span
                    color #FFF
                    font-size .9rem
        .burger
            position absolute
            top 20px
            right 20px
            cursor pointer
            .linea1, .linea2, .linea3
                width 40px
                height 4px
                background-color #FFF
                margin 5px
                transition all .3s ease
            .toggle1
                transform rotate(-45deg) translate(-6.5px,6.5px)
            .toggle2
                opacity 0
            .toggle3
                transform rotate(45deg) translate(-5.5px,-6.5px)

