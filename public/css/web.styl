        .paginationT
            position relative
            width 50%
            display flex
            align-items center
            justify-content center
            .pagination
                position: absolute
                top 50%
                left 83%
                transform translate(-50%, -50%)
                z-index 1
                width 550px
                height 550px
                border-radius 50%
                border 13px solid #000
                transition-property transform
                transition-timing-function ease-out
                // transition-timing-function cubic-bezier(.5,-.5,.5,1.5)
                pointer-events none
                user-select none
                .dot
                    display flex
                    align-items center
                    justify-content center
                    position absolute
                    top 0
                    left 50%
                    transform translate(-50%, -50%)
                    width 100px
                    height 100px
                    border-radius 50%
                    pointer-events auto
                    transition .3s
                    background transparent
                    cursor pointer
                    &:hover
                        transform translate(-50%, -50%) scale(1.3)
                    img
                        position absolute
                        height 180px
                        bottom 0
                        right 0
                        left -20px
                        margin auto
                .item
                    position absolute
                    top 0
                    left 0
                    width 100%
                    height 100%
                    border-radius 50%
                .active
                    .dot
                        transform translate(-50%, -50%) scale(1.7) rotate(90deg)
        .next, .prev
            position relative
            width 100%
            z-index 1
            transition .3s
            user-select none
            width 10%
            height 100%
            cursor pointer
            padding-top 25%

