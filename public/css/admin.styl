.loading
    position fixed
    width 100vw
    height 120vh
    background rgb(0,0,0,1)
    display flex
    justify-content center
    align-items center
    z-index 999999
    top 0
    #loader
        border 2px #3F3F3F solid
        border-radius 50%
        border-top-color #F26E34
        width 90px
        height 90px
        animation spin 1s linear infinite
        z-index 9999
        @keyframes spin
            from
                transform translate(0%, -50%) rotate(0deg)
            to
                transform translate(0%, -50%) rotate(360deg)

