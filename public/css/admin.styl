body
    background rgb(252,252,252)
    margin 0px
    font-family font-regular
    overflow-x hidden !important

.section
    position relative

.glowred
    animation glowred 1s ease-in-out infinite alternate

@-webkit-keyframes glowred
  from
    box-shadow 0px 0px 5px 5px rgba(239, 20, 20, 1)
  to
    box-shadow 0px 0px 5px 9px rgba(239, 20, 20, 1)

.glow0
    animation glow0 1s ease-in-out infinite alternate

@-webkit-keyframes glow0
  from
    box-shadow 0px 0px 20px 10px rgba(255, 255, 255, 1)
  to
    box-shadow 0px 0px 5px 8px rgba(255, 255, 255, 1)

.glow0_blue
    animation glow0_blue 1s ease-in-out infinite alternate

@-webkit-keyframes glow0_blue
  from
    box-shadow 0px 0px 20px 10px #0ff
  to
    box-shadow 0px 0px 5px 8px #0ff
    
.glow1
  text-align center
  -webkit-animation glow 1s ease-in-out infinite alternate
  -moz-animation glow 1s ease-in-out infinite alternate
  animation glow 1s ease-in-out infinite alternate

@-webkit-keyframes glow
  from
    text-shadow 0 0 10px #fff, 0 0 20px #fff, 0 0 30px #e60073, 0 0 40px #e60073, 0 0 50px #e60073, 0 0 60px #e60073, 0 0 70px #e60073
  to
    text-shadow 0 0 20px #fff, 0 0 30px #ff4da6, 0 0 40px #ff4da6, 0 0 50px #ff4da6, 0 0 60px #ff4da6, 0 0 70px #ff4da6, 0 0 80px #ff4da6

.tooltip-inner
    background-color #fff
    color #000
.tooltip.bs-tooltip-right 
    .arrow
        &:before
            border-right-color #fff !important
.tooltip.bs-tooltip-left
    .arrow
        &:before
            border-left-color #fff !important

.tooltip.bs-tooltip-bottom
    .arrow
        &:before
            border-bottom-color #fff !important

.tooltip.bs-tooltip-top
    .arrow
        &:before
            border-top-color #fff !important

